{
    "shebang": {
        "description": "[sh]hebang",
        "prefix": "sh",
        "body": [
            "#!/usr/bin/env ${1|python3,python|}"
        ]
    },
    "if": {
        "description": "[if]",
        "prefix": "if",
        "body": [
            "if ${1:condition}:",
            "\t$0"
        ]
    },
    "ife": {
        "description": "[if] [e]lse",
        "prefix": "ife",
        "body": [
            "if ${1:condition}:",
            "\t${2:print()}",
            "else:",
            "\t$0"
        ]
    },
    "wh": {
        "description": "[wh]ile",
        "prefix": "wh",
        "body": [
            "while ${1:condition}:",
            "\t$0"
        ]
    },
    "whe": {
        "description": "[wh]ile [e]lse",
        "prefix": "whe",
        "body": [
            "while ${1:condition}:",
            "\t${2:print()}",
            "else:",
            "\t$0"
        ]
    },
    "fo": {
        "description": "[fo]r",
        "prefix": "fo",
        "body": [
            "for ${1:variable} in ${2:list}:",
            "\t$0"
        ]
    },
    "foe": {
        "description": "[fo]r [e]lse",
        "prefix": "foe",
        "body": [
            "for ${1:variable} in ${2:list}:",
            "\t${3:print()}",
            "else:",
            "\t$0"
        ]
    },
    "de": {
        "description": "[de]f",
        "prefix": "de",
        "body": [
            "def ${1:function_name}(${2:parameters}):",
            "\t$0"
        ]
    },
    "tr": {
        "description": "[tr]y",
        "prefix": "tr",
        "body": [
            "try",
            "\t${1:print()}",
            "except:",
            "\t$0"
        ]
    },
    "trf": {
        "description": "[tr]y [f]inally",
        "prefix": "trf",
        "body": [
            "try",
            "\t${1:print()}",
            "except:",
            "\t${2:print()}",
            "finally:",
            "\t$0"
        ]
    },
    "wi": {
        "description": "[wi]th",
        "prefix": "wi",
        "body": [
            "with open(\"${1:path/to/file}\", \"${2|r,w|}\") as ${3:file}:",
            "\t$0"
        ]
    },
    "ma": {
        "description": "[ma]in",
        "prefix": "ma",
        "body": [
            "def main():",
            "\t$0"
        ]
    },
    "fr": {
        "description": "[fr]om",
        "prefix": "fr",
        "body": [
            "from ${1:module} import ${2:items}"
        ]
    },
    "im": {
        "description": "[im]port",
        "prefix": "im",
        "body": [
            "import ${1:module}"
        ]
    }
}
